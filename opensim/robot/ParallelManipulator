#ifndef _ROBOT_PARALLELMANIPULATOR_
#define _ROBOT_PARALLELMANIPULATOR_

#include <robot/robot>
#include <robot/Manipulator>


namespace robot {

/**
 * An abstract description of a parallel manipulator
 */
class ParallelManipulator : public Manipulator
{
public:
  virtual Type type() const { return Parallel; }
};


} // robot

#endif
